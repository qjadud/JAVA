import java.util.Scanner;

public class Hello {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int n;
		int cnt = 0;
		
		System.out.print("숫자 입력: ");
		n = input.nextInt();
		
		for(int i = 0; i < n; i++) {
			for(int j = 0; j < n; j++) {
				cnt++;
				System.out.print(cnt + "\t");
			}
			System.out.println();
		}
	}
}
